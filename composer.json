{
    "name": "wamesk/laravel-nova-price-tax-field",
    "description": "A Laravel Nova Price Tax Fields.",
    "keywords": [
        "laravel",
        "nova",
        "price",
        "tax"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Wame\\PriceTaxField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wame\\PriceTaxField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
